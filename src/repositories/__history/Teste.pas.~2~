unit Teste;

interface

uses System.JSON;

implementation

function RetornaArray: TJSONArray;
var lista: TJSONArray;
    medico: TJSONObject;
    counter: integer;
begin
  medico := TJSONObject.Create;
  medico.AddPair('id','1');
  medico.AddPair('nome','João');
  lista  := TJSONArray.Create;
  lista.AddElement(medico);

  result := lista;
end;

end.
